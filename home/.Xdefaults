Xcursor.theme: default

irssi*urgentOnBell:true

vnc*fullScreen: true
vnc*grabKeyboard: true

Rxvt*background:    #000000

!! For Real Transparency
!Rxvt*background:   [80]#000000
!Rxvt*depth:        32

!! For pseudo transparency
Rxvt*transparent: true

Rxvt*foreground:   #00ff00
!Rxvt*font: -xos4-terminus-medium-r-normal--14-140-72-72-c-80-*-u
!Rxvt*font: xft:Terminus:size=10:antialias=true:hinting=true
Rxvt*font: xft:terminus:pixelsize=12:antialias=true:hinting=true
Rxvt*scrollBar: false
Rxvt*borderLess: false
Rxvt*cursorBlink: true
Rxvt*fading: 40
!Rxvt*tintColor: black
Rxvt*shading: 50
Rxvt*visualBell: false

!! For mark-yank-urls plugin
!URxvt.keysym.M-u: perl:mark-yank-urls:activate_mark_mode
!URxvt.underlineURLs: true
!URxvt.perl-lib: /home/possum/.urxvt
!URxvt.perl-ext: selection,mark-yank-urls
!URxvt.urlLauncher: iceweasel
!URxvt.print-pipe: cat > /dev/null

!! Font
!URxvt*font: xft:Terminus:pixelsize=14:antialias=true:hinting=true,xft:Kochi Gothic:antialias=true,xft:Baekmuk Gulim
URxvt*font: xft:Gohufont:pixelsize=14:antialias=true:hinting=true,xft:IPAGothic:pixelsize=14:antialias=true,xft:Baekmuk Gulim

